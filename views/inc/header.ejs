  <script src="/bootstrap-3.1.1-dist/js/bootstrap.js"></script>

  <link rel="stylesheet" type="text/css" href="/dataTables/dataTables.bootstrap.css">
  <script type="text/javascript" language="javascript" src="/dataTables/jquery.dataTables.min.js"></script>
  <script type="text/javascript" language="javascript" src="/dataTables/dataTables.bootstrap.js"></script>
  
  <div id="header_outer" class="navbar navbar-inverse navbar-default navbar-static-top" role="navigation">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-header-collapse" wt-tracker="Header|Collapse|Toggle Navbar">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a id="header_logo" class="navbar-brand" wt-tracker="Header|Logo|Goto Index" href="/" title="Worktile">HINOCLab</a>
        </div>
        <div class="navbar-collapse navbar-header-collapse collapse in" style="height: auto;">

            <ul id="header_menu" class="nav navbar-nav">
                <% if(title=="Home"){ %>
                <li class="active"><a href="home">Home</a></li>
                <li><a href="report">Report</a></li>
                <li><a href="submit">Submit</a></li>
                <li><a href="about">About</a></li>
                <% } else if (title=="Report"){ %>
                  <li><a href="home">Home</a></li>
                  <li class="active"><a href="report">Report</a></li>
                  <li><a href="submit">Submit</a></li>
                  <li><a href="about">About</a></li>
                <% } else if (title=="About"){ %>
                  <li><a href="home">Home</a></li>
                  <li><a href="report">Report</a></li>
                  <li><a href="submit">Submit</a></li>
                  <li class="active"><a href="about" >About</a></li>
                <% } else if (title=="Submit"){ %>
                  <li><a href="home">Home</a></li>
                  <li><a href="report">Report</a></li>
                  <li class="active"><a href="submit">Submit</a></li>
                  <li><a href="about" >About</a></li>
                <% } else{ %>
                  <li><a href="home">Home</a></li>
                  <li><a href="report">Report</a></li>
                  <li><a href="submit">Submit</a></li>
                  <li><a href="about">About</a></li>
                <% } %>
            </ul>

            <ul class="nav navbar-nav navbar-right ng-scope" ng-controller="user_ctrl" id="header_me">
                <li class="divider-vertical"></li>
                
                <li>
                    <a href="/signin" data-toggle="modal" data-target="#Signin" >登录</a>
                </li>
                <li>
                    <a href="/signup" data-toggle="modal" data-target="#Signup">注册</a>
                </li>
                
            </ul>
        </div>
</div>

<div class="modal fade" id="Signin" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
          <h4 class="modal-title" id="myModalLabel">登陆</h4>
        </div>
        <form method="post" action="/signin" >
          <div class="modal-body">
          <div class="row">
            <div class="col-md-3">
            <h4>年级</h4>
            <select name="grade" class="form-control">
              <option>2013级</option>
            </select>
            </div>
            <div class="col-md-3">
              <h4>姓名</h4>
              <select name="name" class="form-control">
              <option>陈添财</option>
              <option>姜逸坤</option>
              <option>李雪寒</option>
              <option>鲍凯狄</option>
              <option>金淼</option>
              <option>陈君</option>
              <option>肖婷</option>
              <option>张静媛</option>
              </select>
            </div>
            <div class="col-md-3">
              <h4>密码</h4>
              <input name="password" type="password" class="form-control" id="exampleInputPassword1" placeholder="Password">
            </div>
          </div>
        </div>
        <div class="modal-footer">
          <button type="submit" class="btn btn-primary">确认登陆</button>
        </div>
      </form>
      </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->

<div class="modal fade" id="Signup" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
          <h4 class="modal-title" id="myModalLabel">登陆</h4>
        </div>
        <form method="post" action="/signin" >
          <div class="modal-body">
          <div class="row">
            <div class="col-md-4">
            <h4>年级</h4>
            <select name="grade" class="form-control">
              <option>2013级</option>
            </select>
            </div>
            <div class="col-md-4">
              <h4>姓名</h4>
              <select name="name" class="form-control">
              <option>陈添财</option>
              <option>姜逸坤</option>
              <option>李雪寒</option>
              <option>鲍凯狄</option>
              <option>金淼</option>
              <option>陈君</option>
              <option>肖婷</option>
              <option>张静媛</option>
              </select>
            </div>
            <div class="col-md-4">
              <h4>密码</h4>
              <input name="password" type="password" class="form-control" id="exampleInputPassword1" placeholder="Password">
            </div>
          </div>
        </div>
        <div class="modal-footer">
          <button type="submit" class="btn btn-primary">确认登陆</button>
        </div>
      </form>
      </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->

</head>